{
    "version": 2,
    "builds": [{
            "src": "index.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [{
            "src": "/",
            "dest": "public/index.html"
        },
        {
            "src": "/css/(.+)",
            "dest": "public/css/(.+)"
        },
        {
            "src": "/(.+)",
            "dest": "/"
        }
    ]
}